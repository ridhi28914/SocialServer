<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.application.social.model.Session" table="Session">

		<id column="SESSIONID" name="sessionId" type="java.lang.Integer">
			<generator class="native" />
		</id>
		<property column="AccessToken" name="accessToken" type="java.lang.String" />
		<property column="UserId" name="userId" type="java.lang.Integer" />
		<property column="Created" name="created" type="java.lang.Long" />
		<property column="Modified" name="modified" type="java.lang.Long" />
		<property column="IsValid" name="isValid" type="java.lang.Boolean" />

	</class>
</hibernate-mapping>